        <PAGEOBJECT XPOS="578.25" YPOS="2024.06078740158" OwnPage="7" ItemID="1266563386" PTYPE="4" WIDTH="300.75" HEIGHT="308.43921259842" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L300.75 0 L300.75 308.439 L0 308.439 L0 0 Z" copath="M0 0 L300.75 0 L300.75 308.439 L0 308.439 L0 0 Z" gXpos="158.722440944882" gYpos="5835.7294488189" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="26 září, Tobě"/>
                <para PARENT="nadpis" ALIGN="2"/>
                <ITEXT FONTSIZE="9" CH="Ahoj."/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <ITEXT FONTSIZE="9" CH="Nevím jak pokračovat. Mám ti popřát hezký den a posléze se zeptat, jak se máš? Zní to hloupě, formálně. A to já už nechci. Nechci se tvářit, že jsem někdo jiný, že Ty jsi někdo jiný. Chci Ti říkat Miltone, ale jsi někdo jiný. Chci sobě říkat Jone, ale jsem někdo jiný."/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <ITEXT FONTSIZE="9" CH="Všichni se zoufale přetvařujeme. Já. Ty, jó, Ty taky. Vy. Oni i ony. V metru (jak já to metro nesnáším!) vídám dennodenně lidi, kteří se tváří nejistě, nebo až příliš sebejistě. A někteří – odpuzující – se maskujou pod tunami něčeho, co jejich tvář ještě víc hyzdí. A mezi nimi jsem já. Odraz ve dveřích vagonu. Každý den vypadám jinak, ale vždy je tam ten unavený úsměv, ty samé boty, ta samá taška. Ten samý člověk. Ale ten se taky přetvařuje. Kdyby to bylo na něm, nebyl by zde. Byl by někde, kde by se cítil dobře, lépe. Dělal by něco, co by k něčemu bylo. "/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <ITEXT FONTSIZE="9" CH="Ale všichni si tím jednou projdeme. Jestli z toho vyjdeme vítězně, nebo poražení, je vcelku jedno. Někdo se najde, setře tu povrchovou špínu a zazáří. Tak mi drž palce. Ať jsem hvězdička na nebi, které září miliardami dalších hvězdiček. Dík."/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <ITEXT FONTSIZE="9" CH="Tak ahoj. Přeji báječný den."/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
                <ITEXT FONTSIZE="9" CH="PS: Hm, nějak jsem vystoupil z role. Jsem Jon. Ty Milton. Tím to končí. Až do další krize, kdy budu chtít být zase sám sebou."/>
                <para PARENT="plain text" ALIGN="0" LINESP="12"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="140.00062992126" YPOS="6460.97133858268" OwnPage="20" ItemID="1266658042" PTYPE="4" WIDTH="339.527559055118" HEIGHT="226.217637795279" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L339.528 0 L339.528 226.218 L0 226.218 L0 0 Z" copath="M0 0 L339.528 0 L339.528 226.218 L0 226.218 L0 0 Z" gXpos="-279.526929133858" gYpos="2014.02456692914" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1266659402"/>
        <PAGEOBJECT XPOS="559.528188976378" YPOS="5825.81952000281" OwnPage="19" ItemID="1266659402" PTYPE="4" WIDTH="339.527559055118" HEIGHT="418.180479997193" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L339.528 0 L339.528 418.18 L0 418.18 L0 0 Z" copath="M0 0 L339.528 0 L339.528 418.18 L0 418.18 L0 0 Z" gXpos="559.528188976378" gYpos="1378.87274834927" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1266658042" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Slovo na počátku Tycha"/>
                <para PARENT="nadpis"/>
                <ITEXT CH="Hvězdy, pod kterými stojí, jej mohou mít za jepici,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="a stejně soudí i skály, na nichž je postaven hrad Perasperad."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Však pro člověka z okolí představuje stálici,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="to díky němu člověk získá přátele i dobrý plat."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" Perasperad není pevnost, které vládne prázdno a zima."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Takové obvinění je jistojistě křivé,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="konstruktivně společenské je zdejší klima."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Všude jsou lidé, Perasperad je i město živé."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Všechny horské mladé generace"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="do Perasperadu zavítají na pár let,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="ty stráví ponořeni do práce a do legrace,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="pak odejdou pryč poznat vzdálenější svět."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Řeč všech měšťanů oslavuje jednoho muže,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="který pevnosti dal přívětivou městskou podobu,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="hradního pána, jehož erb zdobí droboulinká růže,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="a který si svůj titul nechá do a snad i po hrobu."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="To on do žil Perasperadu krev vlil,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="tak není chvíle, kdy nemělo by se nic dít."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Není možné, aby měšťan pasivní byl"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="a pro tento pořádek hrad nikdy nepřestane žít."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Při poslouchání moudrých lidí si na vzdělané hrát?"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Upustit páru stresu v čase na čertoviny?"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="S ostatními měšťany daleko od hradu spát?"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Kdo se chce zorientovat, potřebuje noviny."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Veřejnost myšlence vyslovila podporu,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="kdo by si nepřál do článků shrnutý svůj studentský svět."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Těžší ji bylo přimět, dát své články patřičnému výboru..."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Však nejen noviny se chystaly hradaře oslovit prostřednictvím vět."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Od vzniku města až po jeho milénium,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Každý rok různá skvadra učenců si na nádvoří dala sraz."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="To vše v rámci akce zvané Symposium,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="jejímž účelem bylo pomoc překonat studentům nevědění hráz."/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="Tak či tak, hrad čekaly mediální časy,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="ať už zjišťovat, proč před gamou chrání olovo,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="ať už číst články, či poslouchat pedagogů hlasy,"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="na počátku té divočiny muselo být slovo."/>
                <para PARENT="plain text" ALIGN="1"/>
                <para PARENT="plain text" ALIGN="1"/>
                <ITEXT CH="David Nadrchal"/>
                <trail PARENT="podpis" ALIGN="1"/>
            </StoryText>
        </PAGEOBJECT>
