        <PAGEOBJECT XPOS="157.008188976378" YPOS="2875.21795275591" OwnPage="8" ItemID="1251427386" PTYPE="4" WIDTH="141.732283464567" HEIGHT="249.359490473244" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L141.733 0 L141.733 249.36 L0 249.36 L0 0 Z" copath="M0 0 L141.733 0 L141.733 249.36 L0 249.36 L0 0 Z" gXpos="157.008188976378" gYpos="2875.21795275591" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1269769578" BACKITEM="-1">
            <StoryText>
                <DefaultStyle OpticalMargins="14"/>
                <ITEXT CH="Prostory soukromé střední ScioŠkoly se nacházejí v budově Vysoké školy hotelové v Praze 8, kde se nám také dostává vřelého uvítání ze strany delegace zahrnující zástupkyni ředitele a dva studenty (kteří momentálně mají „službu“, takže se nám sami rádi musejí věnovat – vysvětlují a smějí se). Je ale třeba dodat, že na uskutečnění našeho setkání měli výrazný podíl."/>
                <para PARENT="plain text" ALIGN="3" OpticalMargins="14"/>
                <ITEXT CH="„"/>
                <ITEXT FONT="Playfair Display Italic" CH="Všichni si tu tykáme"/>
                <ITEXT CH="“ je první, co se dozvídáme od Alexandry Kohoutové (zástupkyně ředitele a „průvodkyně vzděláním“). A tak si tykáme."/>
                <para PARENT="plain text" ALIGN="3" OpticalMargins="14"/>
                <ITEXT CH="První střední ScioŠkola navazuje na systém základních škol firmy Scio založené Ondřejem Štefflem (rovněž zakladatel gymnázia PORG). Ač první ScioŠkola vznikla teprve v roce 2015, jejich počet již vzrostl na 9 a otevření dalších (nejen v Praze) je v dohledné budoucnosti."/>
                <para PARENT="plain text" ALIGN="3" OpticalMargins="14"/>
                <ITEXT CH="„"/>
                <ITEXT FONT="Playfair Display Italic" CH="Se základkami jsme jedna parta,"/>
                <ITEXT CH="“ říká Saša, ale zdůrazňuje, že Scio střední není míněna jen jako pokračování pro děti, které z nich vyjdou."/>
                <trail PARENT="plain text" ALIGN="3" OpticalMargins="14"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="140.00062992126" YPOS="2601.1137007874" OwnPage="8" ItemID="1247700186" PTYPE="4" WIDTH="339.527559055118" HEIGHT="100.582992125984" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L339.528 0 L339.528 100.583 L0 100.583 L0 0 Z" copath="M0 0 L339.528 0 L339.528 100.583 L0 100.583 L0 0 Z" gXpos="140.00062992126" gYpos="2601.1137007874" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Scions - fiction"/>
                <para PARENT="nadpis"/>
                <ITEXT CH="Září 2018 přineslo pro ty, kdo hledají ve školství „něco jiného“, další baštu alternativního vzdělávání v podobě první střední ScioŠkoly."/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="Co jsme si přinesli z jednoho dne pod její střechou my?"/>
                <trail PARENT="Podnadpis"/>
